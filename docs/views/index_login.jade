  body
    .loader
      .preloader-wrapper.big.active
        .spinner-layer.spinner-blue
          .circle-clipper.left
            .circle
          .gap-patch
            .circle
          .circle-clipper.right
            .circle
        .spinner-layer.spinner-teal.lighten-1
          .circle-clipper.left
            .circle
          .gap-patch
            .circle
          .circle-clipper.right
            .circle
        .spinner-layer.spinner-yellow
          .circle-clipper.left
            .circle
          .gap-patch
            .circle
          .circle-clipper.right
            .circle
        .spinner-layer.spinner-green
          .circle-clipper.left
            .circle
          .gap-patch
            .circle
          .circle-clipper.right
            .circle
    .mn-content.fixed-sidebar
      header.mn-header.navbar-fixed
        nav.teal.lighten-1
          .nav-wrapper.row
            section.material-design-hamburger.navigation-toggle
              a.button-collapse.show-on-large.material-design-hamburger__icon(href='javascript:void(0)', data-activates='slide-out')
                span.material-design-hamburger__layer
            a(href='index.html')
              .header-title.col.s3.m3
                span.chapter-title Anonymous Pictures
            ul.right.col.s9.m3.nav-right-menu
              li
                a.chat-button.show-on-large(href='javascript:void(0)', data-activates='chat-sidebar')
                  i.material-icons power_settings_new
            form(action='#')
              .file-field.input-field
                .btn
                  span Subir foto
                  input(type='file')
                .file-path-wrapper
                  input.file-path.validate(type='text')
      aside#slide-out.side-nav.white.fixed
        .side-nav-wrapper
          // logo
          img.responsive-img(src='assets/images/logo.png', alt='')
          // Menu lateral izquierdo
          ul.sidebar-menu.collapsible.collapsible-accordion(data-collapsible='accordion')
            li.no-padding.active
              a.waves-effect.waves-grey.active(href='#')
                i.material-icons perm_media
                | INICIO
            li.no-padding
            li.no-padding.active
              a.waves-effect.waves-grey.active(href='login.html')
                i.material-icons person_pin
                | login
            li.no-padding
            li.no-padding.active
              a.waves-effect.waves-grey.active(href='registrarse.html')
                i.material-icons input
                | crear cuenta
            li.no-padding
          .footer
            p.copyright
              | Â© 2017
              br
              | Carlos Fernandez
              br
              |  David Dorta
            a(href='index.html#!') Privacy
            |  &
            a(href='index.html#!') Terms
